<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>
<weblogic-rdbms-jar>
	<weblogic-rdbms-bean>
	<ejb-name>LoginEJB</ejb-name>
    <data-source-name>AdiPool</data-source-name>
    <table-map>
		<table-name>login</table-name>
		<field-map>
			<cmp-field>userid</cmp-field>
			<dbms-column>userid</dbms-column>
		</field-map>
		<field-map>
			<cmp-field>password</cmp-field>
			<dbms-column>password</dbms-column>
		</field-map>
		<field-map>
		    <cmp-field>type</cmp-field>
			<dbms-column>type</dbms-column>
		</field-map>    
	</table-map>
	</weblogic-rdbms-bean>
</weblogic-rdbms-jar>