<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>loginserv</servlet-name>
        <servlet-class>login.loginserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewUserServ</servlet-name>
        <servlet-class>login.NewUserServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllUserServ</servlet-name>
        <servlet-class>login.AllUserServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListServ</servlet-name>
        <servlet-class>login.ListServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CookieLogin</servlet-name>
        <servlet-class>login.CookieLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginserv</servlet-name>
        <url-pattern>/loginserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewUserServ</servlet-name>
        <url-pattern>/NewUserServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllUserServ</servlet-name>
        <url-pattern>/AllUserServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListServ</servlet-name>
        <url-pattern>/ListServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CookieLogin</servlet-name>
        <url-pattern>/CookieLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
